<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--=====================================================-->
    <!-- 启用的通信方式 -->
    <add key="communication" value="1"/>
    <!-- 电子邮件 -->
    <add key="1" value="LogMailApp.Communication.EmailPoster" />
    <!-- WeChat 
    <add key="2" value="" />-->
    <!-- QQ 
    <add key="4" value="" />-->
    <!--=====================================================-->
    <!-- 启用的存储解决方案 -->
    <add key="storage" value="LogMailApp.Storage.FileStorager"/>
    <!--=====================================================-->
    <!-- 目前支持的命令 -->
    <add key="-a" value="LogMailApp.Command.AppendLogCommand" />
    <add key="-f" value="LogMailApp.Command.FileLogCommand" />
    <add key="-p" value="LogMailApp.Command.PostLogCommand" />
    <add key="-ui" value=""/>
  </appSettings>
</configuration>